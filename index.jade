!!!
head
  title 我的幻灯片
  meta(charset='utf-8')
  script(src='http://code.jquery.com/jquery.min.js')
  :coffeescript
    jsonp=(url,cb)->
      $.ajax
        url:url
        dataType:'jsonp'
        success:(data)->
          if data.meta.status==200
            cb null,data.data
          else
            cb data.data,null
    jsonp 'https://api.github.com/repos/layerssss/myslides/contents',(err,data)->
      for file in data
        if file.type=='dir'
          $.ajax
            url:"#{file.name}/"
            success:(data)->
              title=(data.match(/<title>(.*)<\/title>/)||[])[1]
              $("<h2><a href=\"#{file.name}/\">#{title}</a></h2>").appendTo 'body'
            error:()->
              title=file.name
              $("<h2><a href=\"#{file.name}/\">#{title}</a></h2>").appendTo 'body'
body
  h1 我的幻灯片